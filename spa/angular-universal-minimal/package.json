{
  "name": "angular-universal-minimal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run angular-universal-minimal:serve-ssr",
    "serve:ssr": "node dist/angular-universal-minimal/server/main.js",
    "build:ssr": "ng build && ng run angular-universal-minimal:server",
    "prerender": "node ./fetch-routes.js && ng run angular-universal-minimal:prerender",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/platform-server": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@angular/ssr": "^17.0.3",
    "@magnolia/angular-editor": "^1.5.0",
    "domino": "^2.1.6",
    "express": "^4.18.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-eslint/builder": "^17.1.0",
