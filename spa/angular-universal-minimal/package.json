    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "~17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^18.18.13",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jasmine-core": "^5.1.1",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2"
  }
